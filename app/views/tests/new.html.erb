
<h1>Create Test</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@test) do |f| %>
      <%= render 'shared/error_messages',object: f.object %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      
      <%= f.submit "Create Test", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
